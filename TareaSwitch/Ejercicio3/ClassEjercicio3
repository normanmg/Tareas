public class ClassEjercicio3 {
    public void Letra(int letra)
    {
      switch(letra)
      {
          case 'a':case 'A': 
                System.out.println("EXCELENTE");
                break;
            case 'b':case 'B': 
                System.out.println("BUENO");
                break;
            case 'c':case 'C': 
                System.out.println("REGULAR");
                break;
            case 'd':case 'D': 
                System.out.println("MALO");
                break;
            case 'e':case 'E': 
                System.out.println("PESIMO");
                break;
            default: 
                System.out.println("Letra fuera de rango");
                break;
      }
      
    }
}
